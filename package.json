{
  "name": "blog-mobile",
  "version": "0.1.0",
  "description": "blog-mobile",
  "private": true,
  "scripts": {
    "build": "vue-cli-service build --modern --report",
    "build-ci": "vue-cli-service build --modern",
    "debug-cli": "node --inspect-brk ./node_modules/@vue/cli-service/bin/vue-cli-service.js build --modern",
    "lint": "vue-cli-service lint",
    "inspect": "jsinspect src",
    "format": "prettier --config .prettierrc.js --write src/**/*.js",
    "start": "concurrently \"pock\" \"vue-cli-service serve\"",
    "changelog": "conventional-changelog -p angular -i CHANGELOG.md -s"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ta7sudan/blog-mobile.git"
  },
  "keywords": [
    "blog"
  ],
  "author": "ta7sudan",
  "license": "GPL-3.0",
  "bugs": {
    "url": "https://github.com/ta7sudan/blog-mobile/issues"
  },
  "homepage": "https://github.com/ta7sudan/blog-mobile#readme",
  "dependencies": {
    "@lowb/apiz-vue": "^0.1.10",
    "@sentry/browser": "^4.6.5",
    "accessible-nprogress": "^2.0.0",
    "alloylever": "^1.0.4",
    "kooky": "^0.1.3",
    "marked": "^0.6.1",
    "secan": "^0.1.7",
    "vue": "^2.6.10",
    "vue-router": "^3.0.2",
    "vuex": "^3.1.0"
  },
  "devDependencies": {
    "@sentry/webpack-plugin": "^1.6.2",
    "@vue/cli-plugin-babel": "^3.5.1",
    "@vue/cli-plugin-eslint": "^3.5.1",
    "@vue/cli-service": "^3.5.1",
    "babel-eslint": "^10.0.1",
    "babel-plugin-array-includes": "^2.0.3",
    "concurrently": "^4.1.0",
    "conventional-changelog-cli": "^2.0.12",
    "cz-conventional-changelog": "^2.1.0",
    "dotenv-safe": "^6.1.0",
    "eslint": "^5.15.3",
    "eslint-plugin-prettier": "^3.0.1",
    "eslint-plugin-vue": "^5.2.2",
    "fastify-jwt": "^0.9.5",
    "git-revision-webpack-plugin": "^3.0.3",
    "html-tag-attributes-plugin": "^0.1.6",
    "jsinspect": "^0.12.7",
    "lint-staged": "^8.1.5",
    "pock": "^0.3.3",
    "postcss-automath": "^1.0.1",
    "postcss-convertpx": "^0.1.1",
    "postcss-functions": "^3.0.0",
    "postcss-hexrgba": "^1.0.1",
    "postcss-sprites": "^4.2.1",
    "precss": "^3.1.2",
    "preload-font-plugin": "^0.1.4",
    "prettier": "^1.16.4",
    "svg-url-loader": "^2.3.2",
    "vue-template-compiler": "^2.6.10",
    "webpack-deep-scope-plugin": "^1.6.0"
  },
  "gitHooks": {
    "pre-commit": "lint-staged"
  },
  "lint-staged": {
    "*.js": [
      "vue-cli-service lint"
    ],
    "*.vue": [
      "vue-cli-service lint"
    ]
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
